<local:WarningLevelConfig xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                          xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                          xmlns:local="clr-namespace:ToDoMainWindow.Helpers">
    <local:WarningLevelConfig.DefaultBrush>
        <local:WarningLevelBrush Content="Normal" Background="AliceBlue" />
    </local:WarningLevelConfig.DefaultBrush>

    <local:WarningLevelConfig.BrushList>
        <local:WarningLevelBrushList>
            <local:WarningLevelBrush Content="Warning" Hours="2" Background="Bisque" />
        </local:WarningLevelBrushList>
    </local:WarningLevelConfig.BrushList>
</local:WarningLevelConfig>